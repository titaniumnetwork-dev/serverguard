{
  "name": "proxyguard",
  "module": "index.js",
  "type": "module",
  "scripts": {
    "start": "node --env-file=.env src/index.ts",
    "dev": "nodemon --env-file=.env src/index.ts",
    "format": "prettier --config .prettierrc.js --write ./src/",
    "lint": "eslint ./src/",
    "lint:fix": "eslint ./src/ --fix"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/node": "^24.0.3",
    "@types/better-sqlite3": "^7.6.13",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "nodemon": "^3.1.10",
    "eslint": "^9.29.0",
    "prettier": "^3.6.0",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@discordjs/core": "^2.1.1",
    "better-sqlite3": "^12.0.0",
    "discord.js": "^14.20.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1"
  },
  "nodemonConfig": {
		"execMap": {
			"ts": "tsx"
		},
		"ext": "ts,js,json"
	}
}
